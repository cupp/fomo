{
  "atom-text-editor[data-grammar='source fomo']": {
    "|": "fomo:disjunction",
    "&": "fomo:conjunction",
    ": =": "fomo:assignment",
    "@ g": "fomo:genQuant",
    "@ u": "fomo:uniQuant",
    "@ e": "fomo:exiQuant",
    "> =": "fomo:atLeast",
    "< =": "fomo:atMost",
    "/ /": "fomo:boom",
    "!": "fomo:negation",
    "=": "fomo:equivales",
    "! =": "fomo:notEquivales",
    "= =": "fomo:equals",
    "! = =": "fomo:notEquals",
    "> >": "fomo:implies",
    "! > >": "fomo:notImplies",
    "< <": "fomo:follows",
    "! < <": "fomo:notFollows",
    "@ z": "fomo:integer",
    "@ n": "fomo:natural",
    "@ q": "fomo:rational",
    "@ r": "fomo:real",
    "@ b": "fomo:bool"

  }
}
